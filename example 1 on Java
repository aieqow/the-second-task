// Класс узла списка
class Node {
    int data;        // Значение узла
    Node next;       // Ссылка на следующий узел

    Node(int data) {
        this.data = data;
        this.next = null;
    }
}

// Класс односвязного списка
public class SinglyLinkedList {
    private Node head; // Голова списка

    // Метод для добавления элемента в конец списка
    public void add(int data) {
        if (head == null) {
            head = new Node(data); // Создаем новый узел, если список пуст
        } else {
            Node current = head; // Начинаем с головы
            while (current.next != null) {
                current = current.next; // Идем к последнему узлу
            }
            current.next = new Node(data); // Добавляем новый узел в конец
        }
    }

    // Метод для печати элементов списка
    public void printList() {
        Node current = head; // Начинаем с головы
        while (current != null) {
            System.out.print(current.data + " -> "); // Вывод данных
            current = current.next; // Переход к следующему узлу
        }
        System.out.println("null");
    }

    // Пример использования
    public static void main(String[] args) {
        SinglyLinkedList list = new SinglyLinkedList(); // Создаем список
        list.add(1); // Добавляем элементы
        list.add(2);
        list.add(3);
        list.printList(); // Выводим список
    }
}
